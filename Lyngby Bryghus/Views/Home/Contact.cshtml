@using Newtonsoft.Json.Linq
@model JObject
@{
    ViewBag.Title = "Contact";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}

<iframe width="100%" height="400" frameborder="0" style="border:0" src="https://www.google.com/maps/embed/v1/place?q=Ydesvej%204B%2C%20St-14%2C%208500%20Gren%C3%A5%2C%20Denmark&key=AIzaSyCL7nLbeWhn27u8wLNd_qBwfJnO9e6CYsA&maptype=satellite" allowfullscreen></iframe>
<div id="logocontainer">
    <div>
        <img src="LyngbyBryghusLogo.png" />
    </div>
    <div class="redline"></div>
</div>
<div style="max-width: 960px;" class="container" id="contactpage">
    <div class="row">
        <div class="col-lg-6">
            <h2 class=".Contact-title">@Html.Raw(Model["Pages"]["/Home/Contact"]["Title"])</h2><a data-jPath="['Pages']['/Home/Contact']['Title']" data-target=".Contact-title" data-Title="Kontakt Title" class="Edit">Rediger</a>
            <ul>
                <li><span class=".Contact-name">@Html.Raw(Model["Pages"]["/Home/Contact"]["Name"])</span> <a data-jPath="['Pages']['/Home/Contact']['Name']" data-target=".Contact-name" data-Title="Kontakt Navn" class="Edit">Rediger</a></li>
                <li><span class=".Contact-address">@Html.Raw(Model["Pages"]["/Home/Contact"]["Address"])</span> <a data-jPath="['Pages']['/Home/Contact']['Address']" data-target=".Contact-address" data-Title="Kontakt Adresse" class="Edit">Rediger</a></li>
                <li><span class=".Contact-city">@Html.Raw(Model["Pages"]["/Home/Contact"]["City"])</span> <a data-jPath="['Pages']['/Home/Contact']['City']" data-target=".Contact-city" data-Title="Kontakt By" class="Edit">Rediger</a></li>
                <li><span class=".Contact-mail">@Html.Raw(Model["Pages"]["/Home/Contact"]["Mail"])</span> <a data-jPath="['Pages']['/Home/Contact']['Mail']" data-target=".Contact-mail" data-Title="Kontakt E-mail" class="Edit">Rediger</a></li>
                <li><span class=".Contact-phone">@Html.Raw(Model["Pages"]["/Home/Contact"]["Phone"])</span> <a data-jPath="['Pages']['/Home/Contact']['Phone']" data-target=".Contact-phone" data-Title="Kontakt Telefonnummer" class="Edit">Rediger</a></li>
            </ul>
        </div>
        <div class="col-lg-6">
            <h2>Send os en besked</h2>
            <form method="post" action="/Home/Contactform">
                <label>Firmanavn:</label>
                <input id="textinput" name="textinput" type="text" placeholder="" class="form-control input-md">
                <br />
                <label>Navn:</label>
                <input id="textinput" name="textinput" type="text" placeholder="" class="form-control input-md">
                <br />
                <label>E-mailadresse:</label>
                <input id="textinput" name="textinput" type="text" placeholder="" class="form-control input-md">
                <br />
                <label>Tlf. nummer:</label>
                <input id="textinput" name="textinput" type="text" placeholder="" class="form-control input-md">
                <br />
                <label>Besked:</label>
                <br />
                <textarea class="col-lg-5"></textarea>
            </form>
        </div>
    </div>
</div>